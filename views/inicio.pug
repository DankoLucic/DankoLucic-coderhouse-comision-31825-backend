html
    head
        title="Productos"
    body
        h2="Historial Pug JS"
        if hayProductos
            table
                tr 
                    th="ID"
                    th="Tittle"
                    th="Price"
                    th="Thumbnail"
                each producto in productos
                    tr
                        td=producto.id
                        td=producto.tittle
                        td=producto.price
                        td=producto.thumbnail
        else    
            h3="No se encontraron datos"  
