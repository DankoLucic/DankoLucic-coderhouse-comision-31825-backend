<h2>Historial</h2>

<% if(personas.length > 0){  %> 
    <table>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Edad</th>
        </tr>
        <% for(const persona of personas){ %> 
        <tr>
            <td>
                <%-persona.nombre%> 
            </td>
            <td>
                <%-persona.apellido%> 
            </td>
            <td>
                <%-persona.edad%> 
            </td>
        </tr>
        <% }; %> 
    </table>
<% } else { %> 
<h3>No se encontraron datos</h3>
<% } %> 
